#!/bin/bash -e

mkdir -p /etc/phpmyadmin
chmod og+wr /etc/phpmyadmin

echo "Cloning"
git clone git@github.com:phpmyadmin/phpmyadmin.git

cd phpmyadmin
source ${STI_SCRIPTS_PATH}/assemble

echo "Configuration"
wget https://raw.githubusercontent.com/phpmyadmin/docker/master/etc/phpmyadmin/config.inc.php

cd ../


